    enhanced,
    breakable,
    colframe=\mainc{orange},
    colback=\mainc{orange},
    colbacktitle=\mainc{orange},
    coltitle=\accc{orange},
    borderline west={1mm}{0pt}{\accc{orange}},
    sharp corners,
    boxrule=0mm,
    separator sign dash,
    fonttitle=\bfseries}{def}

