\newcommand\accc[1]{#1!50!black}

\newtcbtheorem[number within=section]{example}{Example}{%
    enhanced,
    breakable,
    colframe=\mainc{green},
    colback=\mainc{green},
    colbacktitle=\mainc{green},
    coltitle=\accc{green},
    borderline west={1mm}{0pt}{\accc{green}},
    sharp corners,
    boxrule=0mm,
    separator sign dash,
    fonttitle=\bfseries}{exp}

